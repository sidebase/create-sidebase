{
  "name": "create-sidebase",
  "version": "0.1.0-alpha.9",
  "description": "Create a Nuxt 3 web application with the sidebase stack, like create-t3-app for Nuxt!",
  "scripts": {
    "dev": "vite-node src/",
    "start": "npx",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "build": "rm -rf dist && tsup src/ --format esm",
    "prepack": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidebase/sidebase"
  },
  "bin": {
    "create-sidebase": "./dist/index.js"
  },
  "keywords": [
    "sidebase",
    "create-sidebase",
    "nuxt",
    "typescript",
    "prisma",
    "fullstack"
  ],
  "author": "sidestream-tech <sidebase@sidestream.tech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sidebase/sidebase/issues/new/choose"
  },
  "homepage": "https://github.com/sidebase/sidebase",
  "devDependencies": {
    "@types/node": "^18.11.16",
    "@types/prompts": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "vite-node": "^0.25.8"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "defu": "^6.1.1",
    "giget": "^1.0.0",
    "ora": "^6.1.2",
    "pkg-types": "^1.0.1",
    "prompts": "^2.4.2"
  }
}
